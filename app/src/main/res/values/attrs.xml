<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="ManualInput">
        <attr name="manualInputTitle" format="string" />
        <attr name="manualInputUnit" format="string" />
        <attr name="manualInputEditIme" format="integer">
            <enum name="imeNext" value="0x00000005" />
            <enum name="imeDone" value="0x00000006" />
        </attr>
        <attr name="manualInputEditType" format="integer">
            <enum name="typeNumber" value="0x00000002" />
            <enum name="typeNumberDecimal" value="0x00002002" />
        </attr>
        <attr name="manualInputImage" format="reference" />
    </declare-styleable>

    <declare-styleable name="MeasureItem">
        <attr name="measureItemTitle" format="string" />
        <attr name="measureItemUnit" format="string" />
        <attr name="measureItemValue" format="string" />
    </declare-styleable>

    <declare-styleable name="TabView">
        <attr name="autoFitTabFirst" format="string" />
        <attr name="autoFitTabSecond" format="string" />
        <attr name="autoFitTabThird" format="string" />
    </declare-styleable>

    <declare-styleable name="ChartValueComponent">
        <attr name="chartComponentTitle" format="string" />
        <attr name="chartComponentSubTitle" format="string" />
        <attr name="chartComponentContent" format="string" />
    </declare-styleable>

    <declare-styleable name="ChartValueComponent2">
        <attr name="chartComponent2Unit" format="string" />
        <attr name="chartComponent2TitleVis" format="enum">
            <enum name="visible" value="0" />
            <enum name="invisible" value="4" />
            <enum name="gone" value="8" />
        </attr>
    </declare-styleable>

    <style name="HealthUnit">
        <item name="android:textColor">@color/boulder</item>
        <item name="android:textSize">@dimen/sp_12</item>
    </style>

</resources>